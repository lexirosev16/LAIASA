      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "\"Me llaman la primarosa\"",
              "bold": true
            },
            {
              "text": " "
            },
            {
              "text": "El barbero de Sevilla. ",
              "italic": true
            },
            {
              "text": "Text by Guillermo Perrín (1857-1923) and Miguel de Palacios (1863-1920). Set by Manuel Nieto (1844-1915) and Gerónimo Giménez (1854-1923). "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "\nMe llaman la primarosa"
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "la niña de los amores"
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "por mis ojos tentadores  "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "y esta cara tan graciosa, "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "por  mis labios encendidos "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "como los rojos claveles. "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Do los hombres buscan mieles "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "en abejas convertidos. "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Porque tengo tez morena  "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "que es color de la hermosura "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "y es gallarda mi figura "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "como vara de azucena.¡Ah! "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Porque en mi alma hay un tesoro "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "ya de risa ya de llanto, "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "porque encanto cuando canto "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "y enamoro cuando lloro "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "y me llaman por hermosa "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "los hombres engañadores, "
            }
          ]
        },
        {
          "type": "paragraph",
          "children": [
            {
              "text": "la niña de los amores… "
            }
          ]
